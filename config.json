{
    "openai_api_key": "sk-your-openai-key",
    "openai_organization": "org-your-org-id",
    "openrouter_api_key": "",
    "model_name": "gpt-4o-mini",
    "larger_model_name": "gpt-4o-mini",
    "temperature": 0.0,
    "max_response_tokens": 16383,
    "max_total_tokens": 60000,
    "use_openai_or_openrouter": "openrouter",
    "ocr_settings": {
        "offline": {
            "default_language": "rus",
            "psm_mode": 3,
            "tessdata_dir": "~/tessdata",
            "supported_formats": [
                ".png",
                ".jpg",
                ".jpeg",
                ".tiff",
                ".bmp"
            ]
        },
        "online": {
            "default_prompt": "classic_ocr",
            "max_threads": 5,
            "timeout_seconds": 30,
            "retry_attempts": 3
        }
    },
    "gui_settings": {
        "window_size": "1440x793",
        "theme": "light",
        "auto_save_settings": true,
        "show_image_preview": true,
        "preview_size": 150
    },
    "sorting_options": [
        {
            "name": "Natural (по номерам)",
            "key": "natural"
        },
        {
            "name": "По времени создания",
            "key": "creation_time"
        },
        {
            "name": "По времени изменения",
            "key": "modification_time"
        },
        {
            "name": "Алфавитная сортировка",
            "key": "alphabetical"
        },
        {
            "name": "По размеру файла",
            "key": "file_size"
        }
    ]
}
